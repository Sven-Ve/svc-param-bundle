{
    "name": "svc/param-bundle",
    "description": "Package to handle (set, get, edit) params in a database table",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vetter",
            "email": "git.09o@svenvetter.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "symfony/framework-bundle": "5.4.*|^6",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.11",
        "symfony/validator": "5.4.*|^6",
        "symfony/translation": "5.4.*|^6",
        "symfony/twig-bundle": "5.4.*|^6",
        "symfony/form": "5.4.*|^6"
    },
    "autoload": {
        "psr-4": {
            "Svc\\ParamBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Svc\\ParamBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "5.4.*|^6",
        "symfony/browser-kit": "5.4.*|^6",
        "phpstan/phpstan": "^1"
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit",
        "phpstan": "vendor/bin/phpstan analyse src/ --level 5"
    }
}
